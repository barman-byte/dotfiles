{
    "coc.preferences.formatOnSaveFiletypes": [
        "javascript",
        "typescript",
        "typescriptreact",
        "json",
        "javascriptreact",
        "typescript.tsx",
        "css",
        "scss",
        "html",
        "Markdown"
    ],
    "prettier.eslintIntegration": true,
    "diagnostic.warningSign": "⚠️",
    "diagnostic.errorSign": "❌",
    "diagnostic.hintSign": "⚠️",
    "diagnostic.infoSign": "⚠️",
    "coc.preferences.diagnostic.virtualText": true
}